<% layout("Layouts/boilerplate") %>
    <h3>All Listings</h3>
    <form action="/listings/new" method="get">
      <button>Add new Listing</button>
    </form>
    <ul>
      <%for(let listing of allListings){%>
      <li><a href="/listings/<%=listing._id%>"><%=listing.title%></a></li>
      <%}%>
    </ul>
  </body>
</html>
