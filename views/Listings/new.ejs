<%layout("Layouts/boilerplate")%>
    <h3>All New Listings</h3>
    <form action="/listings" method="POST">

      <input name="listings[title]" type="text" placeholder="Enter title"  />
      <br />

      <textarea name="listings[description]" placeholder="Enter description" ></textarea>
      <br />

      <input type="url" name="listings[image]" placeholder="Enter image URL" />
      <br />

      <input type="number" name="listings[price]" placeholder="Enter price" />
      <br />

      <input type="text" name="listings[location]" placeholder="Enter location"  />
      <br />

      <input type="text" name="listings[country]" placeholder="Enter country"  />
      <br />

      <button>Add</button>
    </form>
  </body>
</html>
